{
  "version": 2,
  "builds": [
    {
      "src": "node_back-end/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "unihospitalar/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/node_back-end/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/unihospitalar/dist/$1"
    }
  ]
}
